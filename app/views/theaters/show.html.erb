<div class="container">	
	<h2 class="text-center title">"<u class="movie_name"><%= @movie.title %></u>"  Movie Available Theaters</h2>
	<% @theaters.each do |theater| %>
		<div class="col-md-4">
			<div class="theaters">
				<h2 class="theater-name"><%= theater.name %></h2>
				<%= link_to "Book Ticket", bookings_path, class: "btn-custom" %>
			</div>
		</div>		
	<% end %>
</div>	